  <script>
    const svgs_with_random_positions = Array.from(Array(10).keys()).map(_ => {
      const svg = document.querySelector('svg').cloneNode(true);
      svg.style.display = 'block';
      svg.style.position = 'absolute';
      svg.style.top = `${Math.random() * 100}vh`;
      svg.style.left = `${Math.random() * 100}vw`;
      svg.style.transform = `translate(-50%, -50%)`;
      svg.style.zIndex = -1;
      svg.style.opacity = 0.05;
      svg.style.width = `${Math.random() * 100}vw`;
      svg.style.height = `${Math.random() * 100}vh`;
      return svg;
    });

    svgs_with_random_positions.forEach(svg => {
      document.body.appendChild(svg);
    });
  </script>
